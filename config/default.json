{
  "protocol": "https",
  "host": "localhost",
  "port": 4040,
  "public": "../public/",
  "paginate": {
    "default": 10,
    "max": 500
  },
  "mongodb": {
    "host": "MONGO_HOST",
    "port": "MONGO_PORT",
    "database": "MONGO_DATABASE"
  },
  "pichasso": {
    "enabled": true,
    "url": "PICHASSO_URL",
    "token": "PICHASSO_TOKEN"
  },
  "authentication": {
    "secret": "",
    "strategies": ["jwt", "local"],
    "path": "/authentication",
    "service": "users",
    "jwt": {
      "header": {
        "typ": "access"
      },
      "audience": "https://yourdomain.com",
      "subject": "anonymous",
      "issuer": "feathers",
      "algorithm": "HS256",
      "expiresIn": "1d"
    },
    "local": {
      "entity": "user",
      "usernameField": "\\username",
      "passwordField": "password"
    }
  }
}
